doctype html
head
  meta(charset='UTF-8')
  meta(name='viewport' content='width=device-width, initial-scale=1.0')
  title DayStar DayCare
  link(rel='stylesheet' href='/sitecss/baby.css')
  link(rel='icon' type='image/png' href='/images/logo.jpeg')
// Main Content
main
nav.navbar
  .container
    a.brand(href='/') Home
    ul.nav-links
      li
        a.nav-link(href='#') 
      li
        a.nav-link(href='/dashboard') Dashboard
  form#babyForm(action="register" method="POST")
    label(for='babyName') Full Name:
    input#name(type='text' name='fullName')
    br
    label(for='gender') Gender:
    select#gender(name='gender')
      option(value='male') Male
      option(value='female') Female
    br
    label(for='age') Age:
    input#age(type='number' name='age')
    br
    label(for='location') Location:
    input#location(type='text' name='location')
    br
    label(for='parent1Name') Parent 1 Name:
    input#name(type='text' name='parent1Name')
    br
    label(for='parent2Name') Parent 2 Name:
    input#name(type='text' name='parent2Name')
    br
    label(for='babyNumber') Baby Number:
    input#babyNumber(type='text' name='babyNumber')
    br
    label(for='contacts') Contacts:
    input#contacts(type='text' name='contacts' )
    br
    input(type='submit' value='Register')
    br
    br

